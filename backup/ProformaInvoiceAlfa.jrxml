	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT row_number() over() as row_no, uc.dividerate, il.c_uom_id as trn_uom, p.c_uom_id as mst_uom, il.qtyentered,
	i.ad_client_id, i.ad_org_id, i.documentno, i.description, i.dateinvoiced, o.documentno as no_order, o.dateordered, o.poreference, sr.name as rayon,
       bp.name2, bpl.npwpname as bp_npwp_name, l.address3 as bp_npwp_address, bpl.npwp as bp_npwp_id,
	   bp.name as bp_name, l.address1 as bp_address, l.isprinted, c.name as bp_city,
	   bpl.name as locationname, uo.name as uomname, p.name as productname, il.qtyentered as qty,
	   il.pricelist as harga_satuan,
	   il.rewarddiscount as rwd_disc, il.additionaldiscount as add_disc, il.regulardiscount as reg_disc,
	   il.priceactual as harga_jual,
	  (SELECT SUM(
		qtyentered /
		case
			when (il.c_uom_id = p.c_uom_id) and (qtyentered > uc.dividerate) then uc.dividerate
			else 1
		 end
		) FROM c_invoiceline WHERE c_invoice_id=i.c_invoice_id) as koli,
	   ((SELECT SUM(qtyentered) FROM c_invoiceline WHERE c_invoice_id=i.c_invoice_id) *
	    (p.weight *
		 case
			when il.c_uom_id = p.c_uom_id then 1
			else uc.dividerate
		 end)
	   ) as tonase,
	  (i.dateinvoiced+pt.netdays) as duedate,
	  (il.pricelist-(il.pricelist*il.regulardiscount)/100) as hrg_jual_proforma
FROM m_inout io
INNER JOIN c_order o ON io.c_order_id = o.c_order_id
INNER JOIN c_invoice i ON o.c_order_id = i.c_order_id
INNER JOIN c_invoiceline il ON i.c_invoice_id = il.c_invoice_id
INNER JOIN c_uom uo ON il.c_uom_id = uo.c_uom_id
INNER JOIN c_salesregion sr ON o.c_salesregion_id = sr.c_salesregion_id
INNER JOIN c_bpartner bp ON io.c_bpartner_id = bp.c_bpartner_id
INNER JOIN c_bpartner_location bpl ON io.c_bpartner_location_id = bpl.c_bpartner_location_id
INNER JOIN c_location l ON bpl.c_location_id = l.c_location_id
INNER JOIN c_city c ON l.c_city_id = c.c_city_id
INNER JOIN m_product p ON il.m_product_id = p.m_product_id
INNER JOIN c_uom_conversion uc ON p.m_product_id = uc.m_product_id
INNER JOIN c_paymentterm pt ON pt.c_paymentterm_id=i.c_paymentterm_id
WHERE i.c_invoice_id = $P{RECORD_ID}]]>
	</queryString>
