	<queryString>
		<![CDATA[SELECT row_number() over(ORDER BY pd.bp_name) as row_no, pd.invoice_salesreg_value, pd.datetrx,
pd.invoice_dateinvoiced, pd.invoice_docno, pd.bp_name, pd.city_description, pd.allocate_invoiceamt,
       pd.allocate_subtotal, pd.bg_amt, pd.tunai_amt, pd.wesel_amt as wesel_htg, --untuk hitung total wesel
       pd.reportdate,
       pd.kupon_amt, pd.bonus_amt, pd.payment_docno,
	--Data Giro
	pd.pdc_documentdate, pd.pdc_duedate, pd.pdc_bankname, pd.pdc_girono,
	--end of Data Giro
       (
	SELECT SUM(pd1.bg_amt)
	FROM tdi_paymentar_detail_v pd1
	INNER JOIN ad_org o1 ON o1.ad_org_id=pd1.ad_org_id
	WHERE pd1.reportdate BETWEEN $P{start_date} AND $P{end_date}
	--dulu pakai salescol_value sekarang ganti invoice_salesreg_value
	AND pd1.invoice_salesreg_id=$P{C_SalesRegion_ID}
	AND pd1.c_charge_id is null
	AND pd1.ad_org_id=$P{AD_Org_ID}
	AND pd1.c_bpartner_id=pd.c_bpartner_id
	AND pd1.pdc_girono=pd.pdc_girono
	--GROUP BY pd1.c_bpartner_id
	GROUP BY pd1.pdc_girono
       ) as bg_amt_tot,
       (
	SELECT SUM(pd1.wesel_amt)
	FROM tdi_paymentar_detail_v pd1
	INNER JOIN ad_org o1 ON o1.ad_org_id=pd1.ad_org_id
	WHERE pd1.reportdate BETWEEN $P{start_date} AND $P{end_date}
	--dulu pakai salescol_value sekarang ganti invoice_salesreg_value
	AND pd1.invoice_salesreg_id=$P{C_SalesRegion_ID}
	AND pd1.c_charge_id is null
	AND pd1.ad_org_id=$P{AD_Org_ID}
	AND pd1.c_bpartner_id=pd.c_bpartner_id
	AND pd1.payment_docno=pd.payment_docno
	--AND pd1.c_paymentallocate_id=pd.c_paymentallocate_id
	GROUP BY pd.payment_docno, pd1.c_bpartner_id
       ) as wesel_amt,
       (
	SELECT count(pd1.reportdate)
	FROM tdi_paymentar_detail_v pd1
          INNER JOIN ad_org o1 ON o1.ad_org_id=pd1.ad_org_id
	WHERE pd1.reportdate BETWEEN $P{start_date} AND $P{end_date}
	--dulu pakai salescol_value sekarang ganti invoice_salesreg_value
	AND pd1.invoice_salesreg_id=$P{C_SalesRegion_ID}
	AND pd1.c_charge_id is null
	AND pd1.ad_org_id=$P{AD_Org_ID}
       ) as count_page, --untuk keperluan tampilan grand total
       TRIM(REPLACE(o.name, 'TDI', '')) as nama_cab --melepas TDI : TDI MADIUN menjadi MADIUN
FROM tdi_paymentar_detail_v pd
INNER JOIN ad_org o ON o.ad_org_id=pd.ad_org_id
WHERE pd.reportdate BETWEEN $P{start_date} AND $P{end_date}
--dulu pakai salescol_value sekarang ganti invoice_salesreg_value
AND pd.invoice_salesreg_id=$P{C_SalesRegion_ID}
AND pd.c_charge_id is null
AND pd.ad_org_id=$P{AD_Org_ID}
ORDER BY pd.bp_name, pd.bg_amt, pd.wesel_amt]]>
	</queryString>
