	<queryString>
		<![CDATA[SELECT *,
    	(SELECT pb.salescol_name
	 FROM tdi_paymentar_branchgiro_v pb
	 INNER JOIN ad_org o ON o.ad_org_id=pb.ad_org_id
	 WHERE pb.c_salesregion_id = $P{C_SalesRegion_ID}
	 AND pb.reportdate BETWEEN $P{start_date} AND $P{end_date}
	 AND pb.ad_org_id=$P{AD_Org_ID}
	 AND pb.salescol_name <> ''
	 LIMIT 1
	) as sales_rep,
       (SELECT TRIM(REPLACE(o1.name, 'TDI', ''))
        FROM ad_org o1 WHERE o1.ad_org_id=$P{AD_Org_ID}) as nama_cab, --melepas TDI : TDI MADIUN menjadi MADIUN
       (SELECT bp1.name
        FROM ad_user u1
        INNER JOIN c_bpartner bp1 ON bp1.c_bpartner_id=u1.c_bpartner_id
        WHERE u1.c_job_id=1000000 AND u1.ad_org_id=$P{AD_Org_ID}) as admin_A,
       (SELECT bp1.name
        FROM ad_user u1
        INNER JOIN c_bpartner bp1 ON bp1.c_bpartner_id=u1.c_bpartner_id
        WHERE u1.c_job_id=1000006 AND u1.ad_org_id=$P{AD_Org_ID}) as admin_B,
       (SELECT bp1.name
        FROM ad_user u1
        INNER JOIN c_bpartner bp1 ON bp1.c_bpartner_id=u1.c_bpartner_id
        WHERE u1.c_job_id=1000005 AND u1.ad_org_id=$P{AD_Org_ID}) as admin_C,
       (SELECT bp1.name
        FROM ad_user u1
        INNER JOIN c_bpartner bp1 ON bp1.c_bpartner_id=u1.c_bpartner_id
        WHERE u1.c_job_id=1000002 AND u1.ad_org_id=$P{AD_Org_ID}) as bm
FROM (
	--Wesel
	SELECT pb.salesreg_value, pb.code, pb.datetrx as datetrx, pb.bg_amt, pb.bg_ho,
		--menurut mb yuyun wesel dihitung berdasarkan no payment bukan per-transaksi
		pb.difference_amt, COALESCE(SUM(pb.wesel_ho),0) as wesel_ho,
		pb.kupon_ho, pb.bonus_amt,
		CASE pb.c_bankaccount_id
		   WHEN 1000014 THEN pb.bp_name
		   ELSE pb.description
		END as description,
		COALESCE(SUM(pb.wesel_ho),0) as total_jml,
	    0 as flag
	FROM tdi_paymentar_branchgiro_v pb
	INNER JOIN ad_org o ON o.ad_org_id=pb.ad_org_id
	WHERE pb.c_salesregion_id = $P{C_SalesRegion_ID}
	AND pb.reportdate BETWEEN $P{start_date} AND $P{end_date}
	AND pb.code LIKE 'c.%'
	AND pb.ad_org_id=$P{AD_Org_ID}
	GROUP BY pb.documentno, pb.salesreg_value, pb.code, pb.datetrx, pb.bg_amt, pb.bg_ho,
	         pb.difference_amt, pb.kupon_ho, pb.bonus_amt, pb.description,
                   pb.c_bankaccount_id, pb.bp_name
UNION ALL
	--Bilyet Giro
	SELECT pb.salesreg_value, pb.code, null, null, SUM(pb.bg_ho),
		   null, null, null, null, null,
		   SUM(pb.bg_ho) as total_jml,
		   1 as flag
	FROM tdi_paymentar_branchgiro_v pb
	INNER JOIN ad_org o ON o.ad_org_id=pb.ad_org_id
	WHERE pb.c_salesregion_id = $P{C_SalesRegion_ID}
	AND pb.reportdate BETWEEN $P{start_date} AND $P{end_date}
	AND pb.code LIKE 'b.%'
	AND pb.ad_org_id=$P{AD_Org_ID}
	GROUP BY pb.salesreg_value, pb.code
UNION ALL
	--Kupon Bonus
          SELECT pb.salesreg_value, pb.code, null, null, null,
		   null, null, SUM(pb.kupon_ho), SUM(pb.bonus_amt), null,
		   COALESCE(SUM(pb.kupon_ho),0)+COALESCE(SUM(pb.bonus_amt),0) as total_jml,
		   2 as flag
	FROM tdi_paymentar_branchgiro_v pb
	INNER JOIN ad_org o ON o.ad_org_id=pb.ad_org_id
	WHERE pb.c_salesregion_id = $P{C_SalesRegion_ID}
	AND pb.reportdate BETWEEN $P{start_date} AND $P{end_date}
	AND pb.code LIKE 'a.%'
	AND pb.ad_org_id=$P{AD_Org_ID}
	GROUP BY pb.salesreg_value, pb.code
UNION ALL
	--Tunai
	SELECT pb.salesreg_value, pb.code, null, null, null,
		   SUM(pb.difference_amt), null, null, null, null,
		   SUM(pb.difference_amt) as total_jml,
		3 as flag
	FROM tdi_paymentar_branchgiro_v pb
	INNER JOIN ad_org o ON o.ad_org_id=pb.ad_org_id
	WHERE pb.c_salesregion_id = $P{C_SalesRegion_ID}
	AND pb.reportdate BETWEEN $P{start_date} AND $P{end_date}
	AND pb.code LIKE 'b.%'
	AND pb.ad_org_id=$P{AD_Org_ID}
	GROUP BY pb.salesreg_value, pb.code, pb.salescol_value, o.name, pb.ad_org_id
) as lap_rinci_piutang
ORDER BY flag, datetrx]]>
	</queryString>
